{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "lib": ["es2020"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "types": ["cypress", "node"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["cypress/*"]
    }
  },
  "include": ["cypress/**/*.ts"],
  "exclude": ["node_modules"]
}